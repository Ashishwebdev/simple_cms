<%= link_to("Back to page",'/testing',:class=> 'action index')%>

<div class="testings new">
	<h2>Create testing</h2>

	<%= form_for @testing, :url => {:action => "create"} do |t| %>
	<table summary= " Testing form fields">
		<tr>
			<th>Name</th>
			<td><%= t.text_field(:name) %></td>
		</tr>
		<tr>
			<th>Position</th>
			<td><%= t.text_field(:position) %></td>
		</tr>
		<tr>
			<th>Visible</th>
			<td><%= t.text_field(:visible) %></td>
		</tr>
	</table>
	<div class="form-buttons">
		<button type="submit" > Submit</button>

	</div>
<% end %>
</div>